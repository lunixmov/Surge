#!name=TikTok Unlock
#!desc=Remove TikTok's regional restrictions.
#!system=ios

[URL Rewrite]
(?<=(carrier|account|sys)_region=)CN(?=&) US 307
(?<=(carrier|account|sys)_region1=)CN(?=&) US 307
(?<=&app_version=)15..(?=.?.?&) 1 307
(?<=\?version_code=)15..(?=.?.?&) 1 307

[MITM]
hostname = %INSERT% *.musical.ly, *.tiktokv.com, *.byteoversea.com, *.snssdk.com,*.pstatp.com
