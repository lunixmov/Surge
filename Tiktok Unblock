#!name=TikTok Unblock
#!desc=Remove TikTok's regional restrictions.
#!system=ios

[URL Rewrite]
(?<=_region=)CN(?=&) US 307
(?<=_region1=)CN(?=&) US 307
(?<=&app_version=)16..(?=.?.?&) 1 307
(?<=\?version_code=)16..(?=.?.?&) 1 307

[MITM]
hostname = %APPEND% *.musical.ly, *.tiktokv.com, *.byteoversea.com, *.snssdk.com
