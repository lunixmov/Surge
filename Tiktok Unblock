#!name=TikTok Unblock
#!desc=Remove TikTok's regional restrictions.
#!system=ios

[URL Rewrite]
(?<=(carrier|current|account|sys)_region=)CN(?=&) JP 307
(?<=_region1=)CN(?=&) JP 307
(?<=&app_version=)15..(?=.?.?&) 1 307
(?<=\?version_code=)15..(?=.?.?&) 1 307

[MITM]
hostname = %INSERT% *.musical.ly, *.tiktokv.com, *.tiktokcdn.com, *.byteoversea.com, *.snssdk.com,*.pstatp.com, *.amemv.com
